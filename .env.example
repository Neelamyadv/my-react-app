# Application Configuration
VITE_APP_NAME=Zyntiq
VITE_APP_VERSION=1.0.0
VITE_APP_URL=https://zyntiq.in

# Payment Gateway Configuration
VITE_RAZORPAY_KEY_ID=rzp_test_your_key_here
VITE_RAZORPAY_SECRET_KEY=your_secret_key_here

# Google OAuth Configuration (Optional)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Supabase Configuration (If using real database)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Content Delivery Network
VITE_CONTENT_CDN_URL=https://your-cdn.com
VITE_CONTENT_API_KEY=your_content_api_key_here

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
VITE_FACEBOOK_PIXEL_ID=your_pixel_id

# Email Service (Optional)
VITE_EMAIL_SERVICE_URL=https://your-email-service.com
VITE_EMAIL_API_KEY=your_email_api_key

# Feature Flags
VITE_ENABLE_GOOGLE_OAUTH=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_OFFLINE_MODE=true

# Development Settings
VITE_ENABLE_DEBUG_LOGGING=false
VITE_ENABLE_MOCK_DATA=true