# Production Environment Configuration
NODE_ENV=production

# Application Configuration
VITE_APP_NAME=Zyntiq
VITE_APP_VERSION=1.0.0
VITE_APP_URL=https://zyntiq.in

# Razorpay Production Configuration
VITE_RAZORPAY_KEY_ID=rzp_live_your_production_key_here
VITE_RAZORPAY_SECRET_KEY=your_production_secret_key_here

# Backend API Configuration
VITE_API_BASE_URL=https://api.zyntiq.in

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_production_google_client_id

# Supabase Configuration
VITE_SUPABASE_URL=your_production_supabase_url
VITE_SUPABASE_ANON_KEY=your_production_supabase_anon_key

# Content Delivery Network
VITE_CONTENT_CDN_URL=https://cdn.zyntiq.in
VITE_CONTENT_API_KEY=your_production_content_api_key

# Analytics
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_FACEBOOK_PIXEL_ID=your_production_pixel_id

# Email Service
VITE_EMAIL_SERVICE_URL=https://email.zyntiq.in
VITE_EMAIL_API_KEY=your_production_email_api_key

# Feature Flags
VITE_ENABLE_GOOGLE_OAUTH=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_OFFLINE_MODE=true

# Development Settings
VITE_ENABLE_DEBUG_LOGGING=false
VITE_ENABLE_MOCK_DATA=false
VITE_LOG_LEVEL=info