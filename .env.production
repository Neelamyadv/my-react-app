# Production Environment Configuration
NODE_ENV=production

# Application Configuration
VITE_APP_NAME=Zyntiq
VITE_APP_VERSION=1.0.0
VITE_APP_URL=https://zyntiq.in

# API Configuration
VITE_API_URL=https://zyntiq.in/api

# Razorpay Production Configuration
VITE_RAZORPAY_KEY_ID=rzp_live_your_production_key_here
VITE_RAZORPAY_SECRET_KEY=your_production_secret_key_here

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Supabase Configuration (if using)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# CDN Configuration
VITE_CDN_URL=https://cdn.zyntiq.in

# Analytics Configuration
VITE_GA_TRACKING_ID=your_ga_tracking_id_here
VITE_GTM_ID=your_gtm_id_here

# Email Service Configuration
VITE_EMAIL_SERVICE_URL=https://api.zyntiq.in/email

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_PWA=true

# Development/Testing (set to false in production)
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_MOCK_DATA=false

# Security Configuration
VITE_ENABLE_CSP=true
VITE_ENABLE_HSTS=true

# Performance Configuration
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHING=true
VITE_CACHE_DURATION=31536000

# Monitoring Configuration
VITE_ENABLE_ERROR_TRACKING=true
VITE_SENTRY_DSN=your_sentry_dsn_here

# Payment Configuration
VITE_PAYMENT_SUCCESS_URL=https://zyntiq.in/payment/success
VITE_PAYMENT_FAILURE_URL=https://zyntiq.in/payment/failure
VITE_PAYMENT_CURRENCY=INR

# Logging Configuration
VITE_LOG_LEVEL=warn
VITE_ENABLE_CONSOLE_LOGS=false